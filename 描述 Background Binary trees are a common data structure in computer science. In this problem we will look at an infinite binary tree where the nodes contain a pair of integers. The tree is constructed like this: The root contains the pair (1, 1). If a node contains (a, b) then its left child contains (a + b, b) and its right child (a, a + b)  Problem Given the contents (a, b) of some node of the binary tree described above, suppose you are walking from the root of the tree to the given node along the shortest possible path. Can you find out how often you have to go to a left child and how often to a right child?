# include<iostream>
using namespace std;
int main()
{
	int a[2*10^9][2]; 
	int i,n,x,y;
	void fun (int x,int y);
	cin>>n;
	for(i=0;i<n;i++)
    {
	   cin>>a[i][0]>>a[i][1];
    }
    for(i=0;i<n;i++)
    {
       cout<<"Scenario"<<" "<< "#"<<i+1<<":"<<"\n";
	   fun(a[i][0],a[i][1]);
	}
    return 0; 
}
int k,j,k1,j1,m,e;
void fun (int x,int y)
{
	
	if(x==1)
 {
 	j=j+y-1;
    cout<<k<<" "<<j<<"\n";
    k=j=k1=j1=m=e=0;
    return ;
 }
   if(y==1)
 {
 	k=k+x-1;
    cout<<k<<" "<<j<<"\n";
    k=j=k1=j1=m=e=0;
    return;
 }
	if(x>y)
	{
		m=x/y;
		k1=x-m*y;
		k=k+m;
		fun(k1,y);
	}
	if(x<y)
	{
		e=y/x;
		j1=y-e*x;
		j=j+e;
		fun(x,j1);
		
	}

   
}
